<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.denise.castro.mymessenger">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_msg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_msg"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyMessenger">
        <activity android:name=".ui.activity.ReceiveMessageActivity"></activity>
        <activity android:name=".ui.activity.CreateMessageActivity" />
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>